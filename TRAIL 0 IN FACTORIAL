public class Main {

    public static void countTrailingZeros(int num) {
        int count = 0;
        for (int i = 5; num / i > 0; i *= 5) {
            count += num / i;
        }
        System.out.println(count);
    }

    public static void main(String[] args) {
        countTrailingZeros(5);
    }
}
