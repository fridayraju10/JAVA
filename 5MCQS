import java.util.*;
public class Main
{
	public static void main(String[] args)
	{
		String a="String";
		String b="CharAndString";
		char[] c=new char[10];
		for(int i=0;i<10;i++)
		{
		    c[i++]=b.charAt(i);
		}
		
		System.out.println(Arrays.toString(c));
	}
}
