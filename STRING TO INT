public class Main 
{
    public static void main(String[] args) 
    {
        String input = "1234";
        int result = 0;

        for (int i = 0; i < input.length(); i++) 
        {
            char c = input.charAt(i);
            result = result * 10 + (c - '0');
        }

        System.out.println("Output: " + result);
    }
}
