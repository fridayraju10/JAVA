public class Main
{
    static int abc(int n)
    {
        if(n == 1)
            return 2;  
        
        int b = abc(n - 1);
        System.out.println("hai" + " ");
        
        return b + 1; 
    }
    
    public static void main(String[] args)
    {
        System.out.println(abc(5));
    }
}
