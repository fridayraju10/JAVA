public class Main {
    public static int raju(int a) {
        return a + 1;
    }
    
    public static int varma(int b) {
        return (raju(b) + 2);
    }
    
    public static void mks1() {  
        mks_1(10);
    }

    public static void mks() {
        System.out.println("Tiger");
        for (char i : "yg".toCharArray()) {
            mks_1(10);
        }
    }

    public static void mks_1(int n) {
        n = n + 5;
        System.out.println(n * 2);
    }

    public static void main(String[] args) {
        for (int i = raju(1); i < varma(2); i++) {
            mks();
        }
    }
}
