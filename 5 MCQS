public class Main {
    public static void main(String[] args) {
        String word = "HKNUDT";

        for (int i = 0; i < word.length(); i++) {
            for (int j = 0; j <= i; j++) {
                System.out.print((char) (word.charAt(j) + 1));
            }
            System.out.println();
        }
    }
}
