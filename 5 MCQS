public class Main 
{
    public static void main(String[] args) 
    {
        int a=8;
        a++;
        a++;
        ++a;
        a--;
        System.out.println(a++ + a--);
        System.out.println(--a - a);
        System.out.println(a);
    }
}
---------------------------------------------------------
public class Main 
{
    public static void love(int a)
    {
        if(a>8)
        return;
        System.out.println("Lubber Pandhu");
        love(a+2);
        System.out.println("Viduthalai");
    }
    
    public static void main(String[] args) 
    {
        for(int i=4;i<7;i++)
        {
            System.out.println("\n----------------");
            love(i);
        }
    }
}

